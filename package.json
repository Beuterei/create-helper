{
  "name": "@beuluis/create-helper",
  "version": "1.1.3",
  "description": "A helper to create a create-something app",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run test-cli -- --name create-test --description test --license MIT",
    "lint": "eslint .",
    "format": "prettier . --check",
    "prepack": "npm run build",
    "build": "tsc --build",
    "test-cli": "rm -Rf test/test-out && ts-node test/test-cli.ts test/test-out",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beuluis/create-helper.git"
  },
  "bugs": {
    "url": "https://github.com/beuluis/create-helper/issues"
  },
  "homepage": "https://github.com/beuluis/create-helper#readme",
  "author": "Luis Beu <me@luisbeu.de> (https://luisbeu.de/)",
  "license": "MIT",
  "dependencies": {
    "@ovyerus/licenses": "^6.4.4",
    "colorette": "^2.0.19",
    "execa": "^6.1.0",
    "inquirer": "^8.2.4",
    "inquirer-search-list": "^1.2.6",
    "liquidjs": "^9.42.0",
    "minimist": "^1.2.6"
  },
  "devDependencies": {
    "@beuluis/eslint-config": "1.0.2",
    "@beuluis/hook-cli": "1.1.2",
    "@beuluis/prettier-config": "2.0.1",
    "@types/inquirer": "9.0.1",
    "@types/minimist": "1.2.2",
    "@types/node": "18.7.18",
    "eslint": "8.23.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "np": "7.6.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "keywords": [
    "create",
    "create-something",
    "create-initializer",
    "initializer",
    "helper"
  ],
  "prettier": "@beuluis/prettier-config",
  "lint-staged": {
    "*.md": [
      "prettier --write"
    ],
    "*.{js,ts,json,yml,yaml}": [
      "prettier --write",
      "eslint"
    ]
  },
  "np": {
    "tests": false,
    "publish": false
  }
}
